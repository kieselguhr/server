<div class="panel-body">
    <div class="table-responsive">
        <table class="table" id="tableWithSearch">
            <thead>
            <tr>
                <th style="width:18%;">UUID</th>
                <th style="width:10%;">Minor</th>
                <th style="width:7%;">Major</th>
                <th style="width:20%;">Message</th>
            </tr>
            </thead>
            <tbody>
            {{#each beacon}}
                <tr>
                    <td class="v-align-middle" id="UUID">
                        {{UUID}}
                    </td>
                    <td class="v-align-middle" id="minor">
                        {{minor}}
                    </td>
                    <td class="v-align-middle" id="major">
                        {{major}}
                    </td>
                    <td class="v-align-middle" id="message">
                        <input type="text" value="{{message}}">
                    </td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    </div>
</div>